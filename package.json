{
  "name": "eosts-contracts",
  "version": "1.0.0",
  "description": "EOS contracts written in typescript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",

    "build:helloworld": "asc contracts/helloworld/*.ts -t build/helloworld.untouched.wast -b build/helloworld.untouched.wasm  --sourceMap --measure --noAssert --use abort=",
    "build:todolist": "asc contracts/todolist/*.ts -t build/todolist.untouched.wast -b build/todolist.untouched.wasm  --sourceMap --measure --noAssert --use abort=",
    "build": "npm run build:helloworld && npm run build:todolist",

    "release:helloworld": "asc contracts/helloworld/*.ts -t build/helloworld.untouched.wast -b build/helloworld.untouched.wasm -O3 --noDebug --sourceMap --measure --noAssert --use abort=",
    "release:todolist": "asc contracts/todolist/*.ts -t build/todolist.untouched.wast -b build/todolist.untouched.wasm -O3 --noDebug --sourceMap --measure --noAssert --use abort=",
    "release": "npm run release:helloworld && npm run release:todolist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "typescript": "^2.9.2"
  }
}
